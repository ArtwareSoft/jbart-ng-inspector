{
  "name": "jbart angular inspector (localhost)",
  "short_name": "jbart localhost angular inspector",
  "description": "jbart angular inspector (localhost)",
  "manifest_version": 2,
  "version": "1.2.3",
  "author": "Yaniv",
  "icons": {
    "128": "jbart_128.png"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "tabs",
    "activeTab",
    "*://*/*"
  ],
  "page_action": {
    "default_icon": "jb_64x64.png"  // optional
  },
  "minimum_chrome_version": "10.0",
  "background": {
    "scripts": ["background.js"]
  }
}